<?xml version="1.0"?>
<config>
    <global>
        <models>
            <NewModule>
                <class>Trening_NewModule_Model</class>
            </NewModule>
        </models>
        <blocks>
            <NewModule>
                <class>Trening_NewModule_Block</class>
            </NewModule>
        </blocks>
        <helpers>
            <NewModule>
                <class>Trening_NewModule_Helper</class>
            </NewModule>
        </helpers>

        <events>
            <customer_login>
                <observers>
                    <NewModule>
                        <type>model</type>
                        <class>NewModule/observer</class>
                        <method>logCustomer</method>
                    </NewModule>
                </observers>
            </customer_login>
        </events>
    </global>
</config>